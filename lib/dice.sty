%%%%%
% Dice icons
%%%%%


\newcommand\SetbackDie{\noindent
\tikz \draw[transform canvas={yshift=-0.2em}] node[shape=rectangle, draw=black, fill=black, very thick, minimum size=1em] {};
}

\newcommand\BoostDie{\noindent
\tikz \draw[transform canvas={yshift=-0.2em}] node[shape=rectangle, draw=black, fill=boost, very thick, minimum size=1em] {};
}

\newcommand\AbilityDie{\noindent
\tikz \draw[transform canvas={yshift=-0.25em}] node[shape=diamond, draw, ultra thick, fill=ability, aspect=0.75, inner sep=0mm, minimum height=1em, minimum width=.8em]{};
}

\newcommand\DifficultyDie{\noindent
\tikz \draw[transform canvas={yshift=-0.25em}] node[shape=diamond, draw, ultra thick, fill=difficulty, aspect=0.75, inner sep=0mm, minimum height=1em, minimum width=.8em]{};
}

\newcommand\ProficiencyDie{\noindent
\tikz \draw[transform canvas={yshift=-0.2em}] node[shape=regular polygon,  regular polygon sides=6, draw, ultra thick, fill=proficiency, inner sep=0mm, minimum size=1em]{};
}


\newcommand\ChallengeDie{\noindent
\tikz \draw[transform canvas={yshift=-0.2em}] node[shape=regular polygon,  regular polygon sides=6, draw, ultra thick, fill=challenge, inner sep=0mm, minimum size=1em]{};
}

%%%%%
% Multiple Dice At Once
%%%%%

\newcommand\Black[1][1]{\foreach \x in {1,...,#1} {\SetbackDie}}
\newcommand\Blue[1][1]{\foreach \x in {1,...,#1} {\BoostDie}}
\newcommand\Green[1][1]{\foreach \x in {1,...,#1} {\AbilityDie}}
\newcommand\Purple[1][1]{\foreach \x in {1,...,#1} {\DifficultyDie}}
\newcommand\Yellow[1][1]{\foreach \x in {1,...,#1} {\ProficiencyDie}}
\newcommand\Red[1][1]{\foreach \x in {1,...,#1} {\ChallengeDie}}


